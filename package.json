    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vercel/analytics": "^1.0.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "currency-codes": "^2.1.0",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.3",
    "export-from-json": "^1.7.2",
    "framer-motion": "^10.12.12",
    "haversine-distance": "^1.2.1",
    "next": "13.4.3",
